{
  "name": "slide-bar",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Digital signage platform for restaurants and bars",
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "start": "./scripts/dev-start.sh",
    "stop": "./scripts/dev-stop.sh",
    "test": "vitest run --config config/vitest.config.ts",
    "test:single": "vitest run --config config/vitest.config.ts",
    "test:watch": "vitest --config config/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config config/vitest.config.ts",
    "test:e2e": "./scripts/test-e2e.sh",
    "test:e2e:single": "./scripts/test-e2e.sh",
    "test:e2e:coverage": "E2E_COVERAGE=true ./scripts/test-e2e.sh",
    "test:e2e:ui": "./scripts/test-e2e-ui.sh",
    "test:e2e:show-report": "./scripts/show-report.sh",
    "type-check": "tsc --noEmit --project config/tsconfig.json",
    "coverage:merge": "node scripts/merge-coverage.js",
    "coverage:check": "node scripts/check-coverage.js",
    "coverage:quick": "pnpm test:coverage && pnpm coverage:check",
    "coverage:all": "pnpm test:coverage && pnpm test:e2e:coverage && pnpm coverage:merge && pnpm coverage:check",
    "lint": "eslint . --config config/eslint.config.js",
    "lint:fix": "eslint . --fix --config config/eslint.config.js",
    "format": "prettier --write . --config config/.prettierrc",
    "format:check": "prettier --check . --config config/.prettierrc"
  },
  "keywords": [
    "digital-signage",
    "saas",
    "restaurant"
  ],
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.21.1",
    "pnpm": ">=10.20.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5"
  },
  "devDependencies": {
    "@esbuild/linux-arm64": "^0.25.12",
    "@eslint/js": "^9.39.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.56.1",
    "@rollup/rollup-darwin-arm64": "^4.53.1",
    "@rollup/rollup-linux-arm64-gnu": "^4.53.1",
    "@tailwindcss/postcss": "4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "nyc": "^17.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.2.2",
    "vite-plugin-istanbul": "7.2.1",
    "vitest": "^3.2.4"
  }
}
